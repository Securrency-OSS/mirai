{
    "type": "scaffold",
    "appBar": {
        "type": "appBar",
        "title": {
            "type": "text",
            "data": "API Demo"
        }
    },
    "body": {
        "type": "scrollView",
        "child": {
            "type": "row",
            "mainAxisAlignment": "center",
            "crossAxisAlignment": "center",
            "children": [
                {
                    "type": "column",
                    "mainAxisAlignment": "center",
                    "crossAxisAlignment": "center",
                    "children": [
                        {
                            "type": "sizedBox",
                            "height": 24
                        },
                        {
                            "type": "elevatedButton",
                            "child": {
                                "type": "text",
                                "data": "Login",
                                "style": {
                                    "color": "#ffffff"
                                }
                            },
                            "style": {
                                "padding": {
                                    "top": 8,
                                    "left": 12,
                                    "right": 12,
                                    "bottom": 8
                                }
                            },
                            "onPressed": {
                                "actionType": "request",
                                "request": {
                                    "url": "https://demo-identity.securrency.com/connect/token",
                                    "method": "post",
                                    "headers": {
                                        "contentType": "application/x-www-form-urlencoded"
                                    },
                                    "contentType": "application/x-www-form-urlencoded",
                                    "data": {
                                        "username": "asim.khan@securrency.com",
                                        "password": "testUser123!",
                                        "client_id": "mobile",
                                        "scope": "openid email roles profile identity.public identity.admin entity securrency.finance securrency.exchange securrency.ens",
                                        "grant_type": "mobile"
                                    }
                                },
                                "actions": {
                                    "200": {
                                        "actionType": "navigate",
                                        "navigationStyle": "push",
                                        "navigationType": "screen",
                                        "widgetJson": {
                                            "type": "exampleScreen",
                                            "assetPath": "assets/json/api_success.json"
                                        }
                                    },
                                    "401": {
                                        "actionType": "navigate",
                                        "navigationStyle": "push",
                                        "navigationType": "screen",
                                        "widgetJson": {
                                            "type": "exampleScreen",
                                            "assetPath": "assets/json/api_failure.json"
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "type": "sizedBox",
                            "height": 24
                        },
                        {
                            "type": "elevatedButton",
                            "child": {
                                "type": "text",
                                "data": "Profile Screen",
                                "style": {
                                    "color": "#ffffff"
                                }
                            },
                            "style": {
                                "padding": {
                                    "top": 8,
                                    "left": 12,
                                    "right": 12,
                                    "bottom": 8
                                }
                            },
                            "onPressed": {
                                "actionType": "navigate",
                                "navigationStyle": "push",
                                "navigationType": "screen",
                                "widgetJson": {
                                    "type": "exampleScreen",
                                    "assetPath": "assets/json/profile_screen_example.json"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}