                                }
                            },
                            {
                                "type": "sizedBox",
                                "width": 20
                            },
                            {
                                "type": "floatingActionButton",
                                "buttonType": "large",
                                "disabledElevation": 0,
                                "child": {
                                    "type": "icon",
                                    "iconType": "material",
                                    "icon": "add"
                                }
                            }
